{
  "name": "i18next-locize-backend",
  "version": "0.0.1",
  "description": "backend layer for i18next using locize service ",
  "main": "./lib/index.js",
  "keywords": [
    "i18next",
    "i18next-backend"
  ],
  "homepage": "https://github.com/i18next/i18next-locize-backend",
  "bugs": "https://github.com/i18next/i18next-locize-backend/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/locize/i18next-locize-backend.git"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "6.0.0",
    "babel-core": "6.4.5",
    "babel-eslint": "5.0.0-beta6",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "babel-register": "6.4.3",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "browserify-istanbul": "0.2.1",
    "chai": "3.5.0",
    "coveralls": "2.11.6",
    "eslint": "2.3.0",
    "i18next": "2.0.26",
    "isparta": "4.0.0",
    "karma": "0.13.19",
    "karma-browserify": "4.4.2",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "0.2.2",
    "karma-cli": "0.1.2",
    "karma-coverage": "0.5.3",
    "karma-coveralls": "1.1.2",
    "karma-expect": "1.1.1",
    "karma-mocha": "0.2.1",
    "karma-phantomjs-launcher": "0.2.3",
    "karma-sinon": "1.0.4",
    "karma-spec-reporter": "0.0.23",
    "mocha": "2.4.5",
    "uglifyify": "3.0.1"
  },
  "scripts": {
    "tdd": "karma start",
    "test": "karma start --singleRun --reporters spec,coverage,coveralls",
    "coverage": "babel-node node_modules/.bin/isparta cover _mocha",
    "coveralls": "npm run coverage && coveralls < coverage/lcov.info",
    "transpile": "babel src -d lib",
    "browserify": "browserify ./src/index.js --outfile ./bin/index.js -g uglifyify -t babelify -s i18nextLocizeBackend",
    "copy": "cp ./bin/index.js ./i18next-locize-backend.min.js",
    "build": "npm run transpile && npm run browserify && npm run copy && git commit -a -m 'updated build output'",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "author": "Jan MÃ¼hlemann <jan.muehlemann@gmail.com> (https://github.com/jamuhl)",
  "license": "MIT"
}
